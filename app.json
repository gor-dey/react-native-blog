{
  "expo": {
    "name": "high-max-text",
    "slug": "high-max-text",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./shared/assets/logo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#27293F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./shared/assets/logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
