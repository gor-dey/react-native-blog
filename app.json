{
  "expo": {
    "name": "high-max-text",
    "slug": "high-max-text",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./shared/assets/logo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#27293F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gordey.highmaxtext"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./shared/assets/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d58f512c-5dc1-41cd-8745-0590ace648bc"
      }
    }
  }
}
